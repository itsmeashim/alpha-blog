<h1>Article Listing Page</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Action</th>
        </tr>
    </thead>

    <tbody><% @articles.each do |article| %>
        <tr>
            <th><%= article.title %></th>
            <th><%= article.description %></th>
            <th>Actions hehe</th>
        </tr>
    </tbody><% end %>
</table>